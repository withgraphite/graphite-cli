{
  "name": "@withgraphite/graphite-cli",
  "version": "0.20.6",
  "license": "None",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "homepage": "https://github.com/withgraphite/graphite-cli",
  "repository": "github:withgraphite/graphite-cli",
  "dependencies": {
    "@withgraphite/graphite-cli-routes": "^0.22.0",
    "@withgraphite/retype": "^0.3.11",
    "@withgraphite/retyped-routes": "^0.3.1",
    "chalk": "^4.1.2",
    "fs-extra": "^10.1.0",
    "open": "^8.4.0",
    "prompts": "^2.4.2",
    "semver": "^7.3.7",
    "strip-ansi": "^6.0.1",
    "tmp": "^0.2.1",
    "yargs": "^17.5.1"
  },
  "pkg": {
    "scripts": "dist/src/**/*.js",
    "assets": "dist/src/commands/**/*",
    "targets": [
      "node16-macos",
      "node16-win",
      "node16-linux"
    ],
    "outputPath": "pkg"
  },
  "scripts": {
    "prepublishOnly": "yarn build",
    "postinstall": "node scripts/node_version.js || node dist/scripts/node_version.js",
    "lint": "eslint src --quiet --ext .ts --cache",
    "check": "tsc --noEmit",
    "build": "rm -rf dist/ && yarn lint && tsc && cp scripts/node_version.js dist/scripts/node_version.js && cp .CHANGELOG.md dist/.CHANGELOG.md",
    "pkg": "yarn build && pkg dist/src/index.js -c package.json --public-packages \"*\"",
    "dev": "./scripts/installation/develop.sh",
    "cli": "node ./dist/src/index.js",
    "test": "tsc && yarn mocha \"dist/test/**/*.test.js\"  --parallel --jobs 8",
    "test-ci": "tsc && nyc --reporter=lcovonly yarn mocha \"dist/test/**/*.test.js\"  --parallel --jobs 8",
    "test-one": "tsc && yarn mocha --parallel --jobs 8",
    "test-grep": "tsc && yarn mocha \"dist/test/**/*.test.js\" -g"
  },
  "nyc": {
    "exclude": ".yarn/**/*"
  },
  "files": [
    "dist"
  ],
  "bin": {
    "graphite": "dist/src/index.js",
    "gt": "dist/src/index.js"
  },
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/chai-as-promised": "^7.1.5",
    "@types/fs-extra": "^9.0.13",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.40",
    "@types/prompts": "^2.4.0",
    "@types/semver": "^7",
    "@types/tmp": "^0.2.3",
    "@types/yargs": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "mocha": "^10.0.0",
    "nock": "^13.2.6",
    "nyc": "^15.1.0",
    "pinst": "^3.0.0",
    "prettier": "^2.7.1",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  }
}
